{
  "configs": [
    {
      "name": "Configuration générale",
      "type": "json", 
      "path": "configs/config.json", 
      "values": [
        {
          "type": "password",
          "name": "token",
          "description": "Token du bot",
          "required": true,
          "default": null, 
          "help": "Rendez-vous sur le portail développeurs de Discord, dans la section \"bot\", cliquez sur \"Copy token\""
        },
        {
          "type": "string", 
          "name": "guildId",
          "description": "ID du serveur", 
          "required": true, 
          "default": "782644006190055486", 
          "help": "https://support.discord.com/hc/en-us/articles/206346498-Where-can-I-find-my-User-Server-Message-ID" 
        },
        {
          "type": "boolean", 
          "name": "Auto-modération",
          "description": "Activer l'automodération ou non", 
          "required": true, 
          "default": true
        },
        {
          "type": "number", 
          "name": "anti-spam",
          "description": "Nombre de messages maximum en 15 secondes avant déclanchement de l'anti-spam",
          "required": false
        }
      ]
    },
    {
      "name": "Configuration des salons",
      "type": "json", 
      "path": "configs/channels.json", 
      "values": [
        {
          "type": "string",
          "name": "logs",
          "description": "Salon de logs du bot",
          "required": true
        }
      ]
    }
  ]
}
